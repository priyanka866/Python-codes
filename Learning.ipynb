    "        print(\"Leap year.\")\n",
    "      else:\n",
    "        print(\"Not leap year.\")\n",
    "    else:\n",
    "        print(\"Leap year.\")\n",
    "  else:\n",
    "    print(\"Not leap year.\")\n",
    "\n",
    "def days_in_month(year, month):\n",
    "    month_days = [31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31] \n",
    "    if month >12 and month <1:\n",
    "        return \"Invalid month\"  \n",
